            <div class="imgSection">
              <img src="./images/insidering.png" alt="">
              <img
                class="centerImgeWhatInside"
                src="./images/insideORg.png"
                alt=""
              >
            </div>
            <div class="cardPara">
              <p>{{ section.settings.text_5 }}</p>
